# 1. Add .env to .gitignore (if not already there)
echo ".env" >> .gitignore

# 2. Remove it from git history going forward (not deleting your local copy)
git rm --cached .env

# 3. Commit the change
git add .gitignore
git commit -m "chore(security): remove .env from repo and ignore going forward"

# 4. Push to GitHub
git push origin main
